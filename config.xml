<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        xmlns:android   = "http://schemas.android.com/apk/res/android"
        id          = "org.wkentaur.etwiktionary"
        versionCode = "6"
        version     = "0.6">
 
    <name>
		English-Estonian miktionary
    </name>

    <name xml:lang="et" dir="ltr"> 
		Inglise-eesti vikisõnastik
    </name>
 
    <description>
		English-Estonian dictionary based on Wiktionary data. Also the data from EKI dictionary is used.	
    </description>
 
    <description xml:lang="et" dir="ltr">
        Vikisõnastikul põhinev inglise-eesti sõnastik. Kasutatud on ka EKI inglise-eesti sõnastikku. 
    </description>

    <icon src="icon.png" width="100" height="100"/>
    
    <preference name="phonegap-version" value="3.7.0" />

    <author email="raul.kern@eesti.ee">
        Raul Kern
    </author>
 
    <feature name="http://api.phonegap.com/1.0/network"/>
	
</widget>
